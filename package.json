{
    "name": "misato-milling-gcode-snippet",
    "displayName": "CNC Milling G-Code Snippet(Gコード日本語版)",
    "description": "CNC Snippet",
    "version": "0.1.0",
    "license": "MIT",
    "publisher": "Misato",
    "author": {
        "name": "Matsumoto Misato"
    },
    "engines": {
        "vscode": "^1.30.2"
    },
    "categories": [
        "Programming Languages",
        "Snippets",
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/mmisato/Gcode.git"
    },
    "icon": "images/logo.png",
    "contributes": {
        "languages": [
            {
                "id": "gcode",
                "aliases": [
                    "gcode",
                    "GCODE",
                    "nc",
                    "syntax",
                    "highlighter",
                    "language"
                ],
                "extensions": [
                    ".nc",
                    ".gcode",
                    ".tap",
                    ".cls",
                    ".apt",
                    ".mpt",
                    ".fnc",
                    ".gc",
                    ".dnc",
                    ".cnc",
                    ".ncf",
                    ".ncc",
                    ".maz",
                    ".hnc",
                    ".knc",
                    ".g00",
                    ".eia"
                ],
                "configuration": "./language/gcode-language.json"
            }
        ],
        "grammars": [
            {
                "language": "gcode",
                "scopeName": "source.gcode",
                "path": "./syntaxes/gcode.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "gcode",
                "path": "./snippets/gcode-snippets.json"
            },
            {
                "language": "gcode",
                "path": "./snippets/mcode-snippets.json"
            },
            {
                "language": "gcode",
                "path": "./snippets/general-snippets.json"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "G-Code",
            "properties": {}
        }
    }
}